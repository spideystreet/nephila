# OpenRouter (LLM)
OPENROUTER_API_KEY=""
OPENROUTER_BASE_URL="https://openrouter.ai/api/v1"
OPENROUTER_MODEL=""
# Embeddings (local — HuggingFace model name)
EMBEDDING_MODEL="intfloat/multilingual-e5-base"

# PostgreSQL
POSTGRES_HOST="localhost"
POSTGRES_PORT="5432"
POSTGRES_USER=""
POSTGRES_PASSWORD=""
POSTGRES_DB=""

# ChromaDB
CHROMA_HOST="localhost"
CHROMA_PORT="8000"
CHROMA_PERSIST_DIR="data/gold/chroma"

# Dagster (must be absolute path)
DAGSTER_HOME="/absolute/path/to/nephila/.dagster"

# SSL — custom CA bundle for French gov sites (Certigna Services CA)
# Build with: scripts/build_ca_bundle.sh  (or see README)
SSL_CERT_FILE="/absolute/path/to/nephila/certs/ca-bundle.pem"

# LangSmith (optional — agent observability)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=""
LANGCHAIN_PROJECT="nephila"
